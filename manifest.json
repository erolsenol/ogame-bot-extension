{
  "name": "my-ex",
  "version": "0.0.1",
  "manifest_version": 3,
  "permissions": ["tabs", "scripting", "webNavigation", "activeTab", "storage"],
  "action": { "default_popup": "index.html" },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "all_frames": true,
      "js": ["src/content.js"],
      "run_at": "document_end"
    }
  ]
}
